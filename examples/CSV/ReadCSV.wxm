/* [wxMaxima: title   start ]
Lesen aus CSV Dateien
   [wxMaxima: title   end   ] */


/* [wxMaxima: input   start ] */
batchload("InitMaxima.mac")$
filename: "Werte.csv"$
/* [wxMaxima: input   end   ] */


/* [wxMaxima: input   start ] */
M: ReadCSV(filename,
    separator>",",
    skip>1
);
/* [wxMaxima: input   end   ] */


/* [wxMaxima: input   start ] */
/* Spalten zu Zeilen */
MM: args(transpose(M))$
/* points als [x1,x2,...], [y1,y2,...] */
pointlist:append(
    [key = "Small points", color=blue, point_size=1,point_type=7, 
        points_joined=true, points(MM[1], MM[2])
    ]
)$

/* [wxMaxima: input   end   ] */


/* [wxMaxima: input   start ] */
ZeichnePunkte(pointlist,0,3)$
/* [wxMaxima: input   end   ] */



